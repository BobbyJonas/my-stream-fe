{
  "name": "my-stream-fe",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "dev:https": "cross-env SSL=true nuxt dev",
    "build": "vue-tsc --noEmit && nuxt build",
    "start": "nuxt start",
    "start:https": "cross-env SSL=true nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".ts,.js,.vue\" --ignore-path .gitignore .",
    "lintfix:js": "eslint --ext \".ts,.js,.vue\" --ignore-path .gitignore . --fix --no-ignore",
    "lint:css": "stylelint '**/*.{css,less,vue}'",
    "lintfix:css": "stylelint '**/*.{css,less,vue}' --fix",
    "lint": "npm run lint:js && npm run lint:css",
    "lintfix": "npm run lintfix:js && npm run lintfix:css",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.12.5",
    "@nuxtjs/composition-api": "^0.32.0",
    "@nuxtjs/dotenv": "^1.4.1",
    "body-parser": "^1.20.0",
    "bootstrap-vue": "^2.22.0",
    "consola": "^2.15.3",
    "core-js": "^3.19.3",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "express-rate-limit": "^6.4.0",
    "express-session": "^1.17.3",
    "mongoose": "^6.4.1",
    "nuxt": "^2.15.8",
    "qs": "^6.11.0",
    "socket.io": "^4.4.1",
    "socket.io-client": "^4.4.1",
    "typescript": "^4.6.4",
    "uuid": "^8.3.2",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vue-smooth-dnd": "^0.8.1",
    "vuex-class": "^0.3.2",
    "vuex-module-decorators": "^1.2.0",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "@nuxt/postcss8": "^1.1.3",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.1.0",
    "@nuxtjs/tailwindcss": "^4.2.1",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vue/runtime-dom": "^3.2.31",
    "autoprefixer": "10.4.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vuejs-accessibility": "^1.1.1",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "less-loader": "^7.3.0",
    "lint-staged": "^12.3.4",
    "postcss-html": "^1.3.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.5.1",
    "stylelint": "^14.5.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-less": "^1.0.5",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "vue-tsc": "^0.35.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && vue-tsc --noEmit"
    }
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --cache --fix",
    "*.{css,less,vue}": "stylelint --fix"
  }
}
